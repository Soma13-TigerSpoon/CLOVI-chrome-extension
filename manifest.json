{
  "name": "CLOVI",
  "description": "이거 뭘로 하지요 ㅋ",
  "version": "0.1.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "service_worker": "src/service/background.js"
  },
  "action": {
    "default_popup": "src/popup/popup.html",
    "default_icon": "assets/images/hello_extensions.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens popup.html"
    }
  }
}
